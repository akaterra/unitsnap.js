{
  "name": "@akaterra.co/unitsnap",
  "version": "1.5.4",
  "description": "Library for snapshot based unit testing",
  "files": [
    "**/*"
  ],
  "scripts": {
    "coverage": "npx nyc --reporter=lcov npm run test",
    "lint": "npx eslint ./src/* --ext=.tsx,.ts --fix",
    "test": "npx ts-node node_modules/jasmine/bin/jasmine JASMINE_CONFIG_PATH=jasmine.json"
  },
  "author": "akaterra",
  "license": "ISC",
  "keywords": [
    "snapshot",
    "testing",
    "unittest"
  ],
  "repository": {
    "type": "git",
    "url": "http://github.com/akaterra/unitsnap.js.git"
  },
  "devDependencies": {
    "@types/jasmine": "^5.1.4",
    "@types/node": "^20.11.20",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "eslint": "^8.48.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "jasmine": "^5.1.0",
    "nyc": "^15.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
