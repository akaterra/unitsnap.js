{
  "name": "@akaterra.co/unitsnap",
  "version": "2.0.0",
  "description": "Library for snapshot based unit testing",
  "main": "./cjs/index.js",
  "module": "./esm/index.js",
  "type": "module",
  "exports": {
    ".": {
      "import": "./esm/index.js",
      "default": "./cjs/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "npx tsc --project tsconfig.cjs.json && npx tsc --project tsconfig.esm.json",
    "coverage": "npx nyc --reporter=lcov npm run test",
    "lint": "npx eslint ./src/* --ext=.ts --fix",
    "test": "node --experimental-specifier-resolution=node --loader ts-node/esm node_modules/jasmine/bin/jasmine JASMINE_CONFIG_PATH=jasmine.json"
  },
  "author": "akaterra",
  "license": "MIT",
  "keywords": [
    "snapshot",
    "testing",
    "unittest"
  ],
  "repository": {
    "type": "git",
    "url": "http://github.com/akaterra/unitsnap.js.git"
  },
  "devDependencies": {
    "@types/jasmine": "^5.1.4",
    "@types/node": "^20.11.20",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "browserify": "^17.0.0",
    "eslint": "^8.48.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "jasmine": "^5.1.0",
    "nyc": "^15.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
